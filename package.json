{
  "name": "rollup-plugin-oss",
  "version": "0.2.3",
  "description": "A rollup plugin to list dependency licenses and zip source files",
  "author": "Axel Meinhardt",
  "license": "MIT",
  "typings": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ameinhardt/rollup-plugin-oss.git"
  },
  "keywords": [
    "rollup",
    "rollup-plugin"
  ],
  "main": "index.js",
  "files": [
    "spdxExpression.js",
    "spdx.json",
    "index.d.ts"
  ],
  "scripts": {
    "lint": "eslint --fix --ext .js,.ts,.json .",
    "precommit": "lint-staged",
    "prebuild": "node build.mjs",
    "build": "tsc -b",
    "build:grammar": "nearleyc spdxExpression.ne -o spdxExpression.ts",
    "bundle": "rollup -c",
    "test": "jest"
  },
  "peerDependencies": {
    "rollup": "^1.0.0 || ^2.0.0 || ^3.0.0"
  },
  "dependencies": {
    "nearley": "^2.20.1",
    "zip-stream": "^4.1.0"
  },
  "devDependencies": {
    "@ameinhardt/eslint-config": "ameinhardt/eslint-config#semver:^0.1.1",
    "@rollup/plugin-commonjs": "^23.0.2",
    "@rollup/plugin-json": "^5.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^9.0.2",
    "@types/jest": "^29.2.0",
    "@types/nearley": "^2.11.2",
    "@types/node": "^18.11.4",
    "eslint": "^8.26.0",
    "jest": "^29.2.1",
    "lint-staged": "^13.0.2",
    "rollup": "^3.2.3",
    "rollup-plugin-oss": "file:.",
    "ts-jest": "^29.0.3",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "eslint-plugin-vue": "9"
      }
    }
  },
  "overrides": {
    "@vue/eslint-config-standard": {
      "eslint-plugin-vue": "^9.0.0"
    }
  }
}

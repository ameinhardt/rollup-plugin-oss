{
  "name": "rollup-plugin-oss",
  "type": "module",
  "version": "0.3.0",
  "description": "A rollup plugin to list dependency licenses and zip source files",
  "author": "Axel Meinhardt",
  "license": "MIT",
  "typings": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ameinhardt/rollup-plugin-oss.git"
  },
  "keywords": [
    "rollup",
    "rollup-plugin"
  ],
  "exports": {
    ".": "./index.js"
  },
  "main": "index.js",
  "files": [
    "spdxExpression.js",
    "spdx.json",
    "index.d.ts",
    "types.d.ts"
  ],
  "scripts": {
    "lint": "eslint --fix --ext .js,.ts,.json .",
    "precommit": "lint-staged",
    "prebuild": "node build",
    "build": "tsc -b",
    "build:grammar": "nearleyc spdxExpression.ne -o spdxExpression.ts",
    "bundle": "rollup -c",
    "clean": "git clean -xdfe node_modules",
    "test": "node --experimental-vm-modules --no-warnings node_modules/jest/bin/jest.js --no-cache"
  },
  "peerDependencies": {
    "rollup": "^1.0.0 || ^2.0.0 || ^3.0.0"
  },
  "dependencies": {
    "nearley": "^2.20.1",
    "zip-stream": "^4.1.0"
  },
  "devDependencies": {
    "@ameinhardt/eslint-config": "ameinhardt/eslint-config#semver:^0.1.1",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/jest": "^29.4.0",
    "@types/nearley": "^2.11.2",
    "@types/node": "^18.15.0",
    "eslint": "^8.36.0",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.0",
    "rollup": "^3.19.1",
    "ts-jest": "^29.0.5",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  }
}
